{

    "manifest_version": 2,
    "name": "UNIGE-VMoodle",
    "version": "1.0.0",

    "description": "Utilisez les forums de discussions Moodle avec désormais plus de facilité !",

    "author": "Nathan Vanson",
    "homepage_url":"https://github.com/NathanVanson/Projet-Applications-Informatiques",

    "browser_action": {
        "default_icon": "icons/unige-32.png",
        "default_title": "Visualization on Moodle Unige",
        "default_popup": "popup.html"
    },

    "icons": {
        "32": "icons/unige-32.png",
        "240": "icons/unige-240.png",
        "512": "icons/unige-512.png"
    },
    
    "permissions": [
        "webNavigation",
        "activeTab"
    ],

    "background": {
        "scripts": ["background.js"],
        "persistent": true
    }
}
